{
  "tools": {},
  "agents": {},
  "worktrees": {
    "apply": {
      "integration_mode": "commit_pr"
    },
    "enabled": true,
    "strategy": "bare_control_siblings"
  },
  "defaults": {
    "testing": {
      "tdd": {
        "workflow": "red-green-refactor"
      },
      "coverage": {
        "target_percent": 100,
        "minimum_percent": 80
      },
      "mocking": {
        "policy": "avoid"
      }
    }
  }
}
